{
  "name": "japanese-vocab-website",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run config environment.prod && ng serve -c production",
    "netlify:install": "npm i @datorama/akita@8 --legacy-peer-deps && npm run fix",
    "ni": "npm run netlify:install",
    "dev": "npm run config environment.development && ng serve -c development",
    "build": "npm run config environment.prod && ng build -c production",
    "watch": "npm run config environment.development && ng build --watch --configuration development",
    "config": "ts-node src/environments/set-env.ts",
    "test": "ng test",
    "fix": "npm audit fix --force"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.0.0",
    "@angular/cdk": "^16.2.12",
    "@angular/common": "^16.0.0",
    "@angular/compiler": "^16.0.0",
    "@angular/core": "^16.0.0",
    "@angular/forms": "^16.0.0",
    "@angular/material": "^16.2.12",
    "@angular/platform-browser": "^16.0.0",
    "@angular/platform-browser-dynamic": "^16.0.0",
    "@angular/router": "^16.0.0",
    "@apollo/client": "^3.0.0",
    "@datorama/akita": "^7.1.1",
    "@datorama/akita-ng-entity-service": "^7.0.0",
    "@datorama/akita-ng-router-store": "^7.0.0",
    "@ngneat/until-destroy": "^10.0.0",
    "apollo-angular": "^5.0.2",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "graphql": "^16.8.1",
    "rxjs": "~7.8.0",
    "sweetalert2": "^11.6.13",
    "tslib": "^2.3.0",
    "uuid": "^9.0.1",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.2",
    "@angular/cli": "^16.2.10",
    "@angular/compiler-cli": "^16.0.0",
    "@types/jasmine": "~4.3.0",
    "@types/jquery": "^3.5.27",
    "@types/number-to-words": "^1.2.3",
    "@types/uuid": "^9.0.7",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.0.2"
  }
}
